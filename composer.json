{
  "name": "yzh52521/think-glide",
  "description": "ThinkPHP6 adapter for using Glide image manipulation library",
  "type": "library",
  "keywords": ["thinkphp", "glide", "think-glide"],
  "license": "MIT",
  "authors": [
    {
      "name": "yzh52521",
      "email": "396751927@qq.com"
    }
  ],
  "require": {
    "php": ">=7.2.0",
    "league/glide": "^2.0",
    "symfony/options-resolver": "^5.0|^6.0"
  },
  "require-dev": {
    "topthink/framework": "^6.0"
  },
  "autoload": {
    "psr-4": {
      "think\\": "src/"
    },
    "files": [
      "src/helper.php"
    ]
  },
  "extra": {
    "think": {
      "config":{
        "glide": "src/config.php"
      }
    }
  }
}
